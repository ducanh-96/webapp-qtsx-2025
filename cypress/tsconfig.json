{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "types": ["cypress"],
    "isolatedModules": false
  },
  "include": ["../node_modules/cypress", "./**/*.ts", "./**/*.tsx"],
  "exclude": [
    "../src/**/__tests__/**/*",
    "../src/**/*.test.ts",
    "../src/**/*.test.tsx",
    "../src/**/*.spec.ts",
    "../src/**/*.spec.tsx"
  ]
}
